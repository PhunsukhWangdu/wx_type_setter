mkdir -p public src/components src/views src/router src/store src/styles src/utils src/composables

touch public/index.html src/main.js src/App.vue src/router/index.js src/views/EditorView.vue 

# 删除旧的 node_modules 和 package-lock.json
rm -rf node_modules package-lock.json

# 重新安装依赖
npm install

# 启动开发服务器
npm run dev 